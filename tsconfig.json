{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": false,
		"removeComments": false,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"target": "es6",
		"outDir": "out-tsc",
		"baseUrl": "./",
		"incremental": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"sourceMap": false
	},
	"exclude": ["node_modules", "dist"],
	"include": ["./commands/*.ts", "./src/**/*.ts"]
}
{
  "name": "planktons-office",
  "version": "1.0.1c",
  "description": "A self-written bot based on Discord API with TS superset for a quite popular streamer",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node app/index.ts",
    "deploy-cmd": "ts-node app/deploy-commands.ts",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "prisma-init": "prisma init --datasource-provider sqlite",
    "prisma-migrate": "npx prisma migrate dev --name init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Falcion/Planktons-Office.git"
  },
  "prisma": {
    "schema": "database/prisma/schema.prisma"
  },
  "author": "Falcion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Falcion/Planktons-Office/issues"
  },
  "homepage": "https://github.com/Falcion/Planktons-Office#readme",
  "dependencies": {
    "@discordjs/rest": "^1.2.0",
    "@open-wc/building-rollup": "^2.0.2",
    "@prisma/client": "^4.5.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "crc": "^4.1.1",
    "deepmerge": "^4.2.2",
    "discord.js": "^14.4.0",
    "dotenv": "^16.0.2",
    "fs-extra": "^10.1.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.9",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.24.0",
    "prettier": "2.7.1",
    "prisma": "^4.5.0",
    "typescript": "^4.8.4"
  }
}

{
  "name": "planktons-office",
  "version": "1.0.2",
  "description": "A self-written bot based on Discord API written with Typescript super-set which handles applications within administration's check system",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "deploy-cmd": "ts-node deploy-commands.ts",
    "compiled-start": "node out/index.js",
    "compiled-deploy-cmd": "node out/deploy-commands.js",
    "prisma-init": "prisma init --datasource-provider sqlite",
    "prisma-migrate": "npx prisma migrate dev --name init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Falcion/Planktons-Office.git"
  },
  "keywords": [
    "typescript",
    "discord",
    "discord-bot",
    "applications"
  ],
  "author": "Falcion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Falcion/Planktons-Office/issues"
  },
  "homepage": "https://github.com/Falcion/Planktons-Office#readme",
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "@prisma/client": "^4.6.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "crc": "^4.1.1",
    "deepmerge": "^4.2.2",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "fs-extra": "^10.1.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0",
    "sqlite3": "^5.1.5"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/mdurl": "^1.0.2",
    "@types/node": "^18.11.9",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "jsdoc": "^4.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "prisma": {
    "schema": "database/prisma/schema.prisma"
  }
}
